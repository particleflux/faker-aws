{
    "name": "particleflux/faker-aws",
    "description": "A provider for AWS related data for fakerphp/faker",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "fakerphp/faker": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Linke",
            "email": "particleflux@gmail.com",
            "homepage": "https://particleflux.codes"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "particleflux\\FakerAWS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "particleflux\\FakerAWS\\tests\\": "tests"
        }
    }
}
